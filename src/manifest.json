{
  "name": "GitHub Monorepo Navbar",
  "version": "1.0.0",
  "description": "Adds a navbar with buttons for each package in a monorepo",
  "homepage_url": "https://github.com/jdf221/GithubMonorepoNavbar",
  "manifest_version": 2,
  "icons": {
    "128": "icon.png"
  },
  "permissions": ["https://github.com/*", "https://api.github.com/*"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["contentScript.js"],
      "run_at": "document_end"
    }
  ]
}
